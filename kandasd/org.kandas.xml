<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kandas">
    <signal name="initEnvironmentInfo">
        <arg name="state" type="i" direction="out"/>
    </signal>
    <signal name="initDeviceInfo">
      <arg name="device" type="s" direction="out"/>
    </signal>
    <signal name="initSlotInfo">
      <arg name="slot" type="i" direction="out"/>
      <arg name="device" type="s" direction="out"/>
      <arg name="state" type="i" direction="out"/>
    </signal>
    <signal name="initInfoComplete">
    </signal>
    <signal name="slotAdded">
      <arg name="slot" type="i" direction="out"/>
      <arg name="device" type="s" direction="out"/>
      <arg name="state" type="i" direction="out"/>
    </signal>
    <signal name="slotChanged">
      <arg name="slot" type="i" direction="out"/>
      <arg name="device" type="s" direction="out"/>
      <arg name="state" type="i" direction="out"/>
    </signal>
    <signal name="slotRemoved">
      <arg name="slot" type="i" direction="out"/>
      <arg name="device" type="s" direction="out"/>
    </signal>
    <signal name="deviceAdded">
      <arg name="device" type="s" direction="out"/>
    </signal>
    <signal name="deviceRemoved">
      <arg name="device" type="s" direction="out"/>
    </signal>
    <signal name="environmentChanged">
      <arg name="state" type="i" direction="out"/>
    </signal>
    <method name="registerClient">
    </method>
    <method name="unregisterClient">
    </method>
    <method name="startDriver">
    </method>
    <method name="stopDriver">
    </method>
    <method name="connectSlot">
      <arg name="slot" type="i" direction="in"/>
      <arg name="readOnly" type="b" direction="in"/>
    </method>
    <method name="disconnectSlot">
      <arg name="slot" type="i" direction="in"/>
    </method>
    <method name="connectDevice">
      <arg name="device" type="s" direction="in"/>
      <arg name="readOnly" type="b" direction="in"/>
    </method>
    <method name="disconnectDevice">
      <arg name="device" type="s" direction="in"/>
    </method>
    <method name="refreshData">
    </method>
    <method name="initClient">
    </method>
    <method name="engineVersion">
      <arg type="s" direction="out"/>
    </method>
  </interface>
</node>
